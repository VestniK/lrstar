   /****************/
   /*              */
   /*  PL1 Tokens  */
   /*              */
   /****************/

   <error>         => error();
   <identifier>    => lookup();
   <character>     => lookup();
   <string>        => lookup();
   <integer>       => lookup();
   <picture>       => lookup();



   /*****************/
   /*               */
   /*  PL1 Grammar  */
   /*               */
   /*****************/

   Start
	   -> TranUnit <eof>

   TranUnit
	   ->
	   -> TranList

   TranList
	   -> TranItem
	   -> TranList TranItem

   TranItem
	   -> Procedure



   /***************/
   /*             */
   /*  PROCEDURE  */
   /*             */
   /***************/

   Procedure
	   -> PfixList PrcdSpec PrcdUnitList PrcdEnd

   PrcdSpec
	   -> ProcStmt

   PrcdUnitList
	   ->
	   -> PrcdUnitList PrcdUnitItem

   PrcdUnitItem
	   -> Unit

   PrcdEnd
	   -> PrcdSnamList PrcdEndStmt

   PrcdSnamList
	   ->
	   -> PrcdSnamList PrcdSnamItem

   PrcdSnamItem
	   -> StmtName

   PrcdEndStmt
	   -> EndStmt




   /**********/
   /*        */
   /*  UNIT  */
   /*        */
   /**********/

   Unit
	   -> PfixList UnitDeclStmt
	   ->          UnitDeclStmt
	   -> PfixList UnitDfltStmt
	   ->          UnitDfltStmt
	   -> PfixList UnitEntryStmt
	   -> PfixList UnitFmtStmt
	   ->          UnitPrcdStmt
	   ->          UnitExecUnit

   UnitDeclStmt
	   -> DeclareStmt

   UnitDfltStmt
	   -> DefaultStmt

   UnitEntryStmt
	   -> EntryStmt

   UnitFmtStmt
	   -> FormatStmt

   UnitPrcdStmt
	   -> Procedure

   UnitExecUnit
	   -> ExecUnit



   /***************/
   /*             */
   /*  EXEC UNIT  */
   /*             */
   /***************/

   ExecUnit
	   -> PfixList ExecGroup
	   ->          ExecGroup
	   -> PfixList ExecBlock
	   ->          ExecBlock
	   -> PfixList ExecOnStmt
	   ->          ExecOnStmt
	   -> PfixList ExecIfStmt
	   ->          ExecIfStmt
	   -> PfixList ExecSnglStmt
	   ->          ExecSnglStmt

   ExecGroup
	   -> Group

   ExecBlock
	   -> Block

   ExecOnStmt
	   -> OnStmt

   ExecIfStmt
	   -> IfStmt

   ExecSnglStmt
	   -> AllocateStmt
	   -> AssignmentStmt
	   -> CallStmt
	   -> CloseStmt
	   -> DeleteStmt
	   -> ExecStmt
	   -> FreeStmt
	   -> GetStmt
	   -> GotoStmt
	   -> LocateStmt
	   -> NullStmt
	   -> OpenStmt
	   -> PutStmt
	   -> ReadStmt
	   -> ReturnStmt
	   -> RevertStmt
	   -> RewriteStmt
	   -> SignalStmt
	   -> StopStmt
	   -> WriteStmt



   /**************/
   /*            */
   /*  BAL UNIT  */
   /*            */
   /**************/

   BalUnit
	   -> PfixList BalExecStmt
	   ->          BalExecStmt
	   -> PfixList BalGroup
	   ->          BalGroup
	   -> PfixList BalBlock
	   ->          BalBlock
	   -> PfixList BalOnStmt
	   ->          BalOnStmt
	   -> PfixList BalClause BalUnit BalElse BalUnit
	   ->          BalClause BalUnit BalElse BalUnit

   BalExecStmt
	   -> ExecSnglStmt

   BalGroup
	   -> Group

   BalBlock
	   -> Block

   BalOnStmt
	   -> OnStmt

   BalClause
	   -> IfClause

   BalElse
	   -> ELSE



   /***********/
   /*         */
   /*  GROUP  */
   /*         */
   /***********/

   Group
	   -> GrpDoStmt GrpUnitList GrpEnd

   GrpDoStmt
	   -> DoStmt

   GrpUnitList
	   ->
	   -> GrpUnitList GrpUnitItem

   GrpUnitItem
	   -> Unit

   GrpEnd
	   -> GrpSnamList GrpEndStmt

   GrpSnamList
	   ->
	   -> GrpSnamList GrpSnamItem

   GrpSnamItem
	   -> StmtName

   GrpEndStmt
	   -> EndStmt



   /***********/
   /*         */
   /*  BLOCK  */
   /*         */
   /***********/

   Block
	   -> BlkDoStmt BlkUnitList BlkEnd

   BlkDoStmt
	   -> BeginStmt

   BlkUnitList
	   ->
	   -> BlkUnitList BlkUnitItem

   BlkUnitItem
	   -> Unit

   BlkEnd
	   -> BlkSnamList BlkEndStmt

   BlkSnamList
	   ->
	   -> BlkSnamList BlkSnamItem

   BlkSnamItem
	   -> StmtName

   BlkEndStmt
	   -> EndStmt



   /**************/
   /*            */
   /*  ALLOCATE  */
   /*            */
   /**************/

   AllocateStmt
	   -> AlocInit AlocList AlocDone

   AlocInit
	   -> ALLOCATE
	   -> ALLOC

   AlocList
	   -> AlocItem
	   -> AlocList ',' AlocItem

   AlocItem
	   -> AlocName AlocSet AlocIn
	   -> AlocName AlocSet
	   -> AlocName         AlocIn
	   -> AlocName

   AlocName
	   -> <identifier>

   AlocSet
	   -> SET '(' AlocSet1 ')'

   AlocSet1
	   -> Reference

   AlocIn
	   -> IN '(' AlocIn1 ')'

   AlocIn1
	   -> Reference

   AlocDone
	   -> ';'



   /****************/
   /*              */
   /*  ASSIGNMENT  */
   /*              */
   /****************/

   AssignmentStmt
	   -> /* AsgnInit */  AsgnList '=' AsgnExpr AsgnTail AsgnDone
   /*
   AsgnInit
	   ->
   */
   AsgnList
	   -> AsgnItem
	   -> AsgnList ',' AsgnItem

   AsgnItem
	   -> Reference

   AsgnExpr
	   -> Expr

   AsgnTail
	   ->
	   -> ',' BY NAME

   AsgnDone
	   -> ';'



   /***********/
   /*         */
   /*  BEGIN  */
   /*         */
   /***********/

   BeginStmt
	   -> BgnInit BgnBody BgnDone

   BgnInit
	   -> BEGIN

   BgnBody
	   ->
	   -> OPTIONS '(' BgnOptList ')'

   BgnOptList
	   -> BgnOptItem
	   -> BgnOptList ',' BgnOptItem

   BgnOptItem
	   -> <identifier>

   BgnDone
	   -> ';'



   /**********/
   /*        */
   /*  CALL  */
   /*        */
   /**********/

   CallStmt
	   -> CallInit CallBody CallDone

   CallInit
	   -> CALL

   CallBody
	   -> Reference

   CallDone
	   -> ';'



   /***********/
   /*         */
   /*  CLOSE  */
   /*         */
   /***********/

   CloseStmt
	   -> CloseInit CloseList CloseDone

   CloseInit
	   -> CLOSE

   CloseList
	   -> CloseItem
	   -> CloseList ',' CloseItem

   CloseItem
	   -> FILE '(' CloseFile ')' CloseTail
	   -> FILE '(' CloseFile ')'

   CloseFile
	   -> Reference

   CloseTail
	   -> ENVIRONMENT '(' CloseEnvList ')'
	   -> ENV         '(' CloseEnvList ')'

   CloseEnvList
	   -> CloseEnvItem
	   -> CloseEnvList ',' CloseEnvItem

   CloseEnvItem
	   -> <identifier>

   CloseDone
	   -> ';'



   /*************/
   /*           */
   /*  DECLARE  */
   /*           */
   /*************/

   DeclareStmt
	   -> DeclInit DeclList DeclDone

   DeclInit
	   -> DECLARE
	   -> DCL

   DeclList
	   -> DeclItem
	   -> DeclList ',' DeclItem

   DeclItem
	   -> DeclLevl DeclName DeclDimSfix DeclAttrList
	   -> DeclLevl DeclList DeclDimSfix DeclAttrList

   DeclLevl
	   -> <integer>

   DeclName
	   -> <identifier>

   DeclDimSfix
	   ->
	   -> '(' DeclBndList ')'

   DeclBndList
	   -> DeclBndItem
	   -> DeclBndList ',' DeclBndItem

   DeclBndItem
	   -> BoundPair

   DeclAttrList
	   -> DeclAttrItem
	   -> DeclAttrList DeclAttrItem

   DeclAttrItem
	   -> Attribute

   DeclDone
	   -> ';'



   /*************/
   /*           */
   /*  DEFAULT  */
   /*           */
   /*************/

   DefaultStmt
	   -> DfltInit DfltBody DfltDone

   DfltInit
	   -> DEFAULT
	   -> DFT

   DfltBody
	   -> NONE
	   -> SYSTEM
	   -> DfltHead DfltTail

   DfltHead
	   -> '(' DfltExpr ')'

   DfltExpr
	   -> DfltExpr '|' DfltExpr3
	   ->              DfltExpr3

   DfltExpr3
	   -> DfltExpr3 '&' DfltExpr2
	   ->               DfltExpr2

   DfltExpr2
	   -> '~' DfltExpr2
	   ->     DfltExpr1

   DfltExpr1
	   -> '(' DfltExpr ')'
	   -> DfltAttrKwrd
	   -> DfltRange

   DfltAttrKwrd
	   -> AttrKwrd

   DfltRange
	   -> DfltRngHead '(' DfltRngSpec ')'

   DfltRngHead
	   -> RANGE

   DfltRngSpec
	   -> '*'
	   -> <identifier>
	   -> DfltRng1 ':' DfltRng2

   DfltRng1
	   -> <character>

   DfltRng2
	   -> <character>

   DfltTail
	   -> ERROR
	   -> DfltAttrList

   DfltAttrList
	   -> DfltAttrItem
	   -> DfltAttrList ',' DfltAttrItem
	   -> DfltAttrList     DfltAttrItem

   DfltAttrItem
	   -> Attribute

   DfltDone
	   -> ';'



   /************/
   /*          */
   /*  DELETE  */
   /*          */
   /************/

   DeleteStmt
	   -> DelInit DelFile DelKey  DelDone
	   -> DelInit DelFile         DelDone

   DelInit
	   -> DELETE

   DelFile
	   -> FILE '(' DelFile1 ')'

   DelFile1
	   -> Reference

   DelKey
	   -> KEY '(' DelKey1 ')'

   DelKey1
	   -> Expr

   DelDone
	   -> ';'



   /*********/
   /*       */
   /*  DO   */
   /*       */
   /*********/

   DoStmt
	   -> DoInit         DoDone
	   -> DoInit DoWhile DoDone
	   -> DoInit DoSpec  DoDone

   DoInit
	   -> DO

   DoWhile
	   -> WHILE '(' DoWhile1 ')'

   DoWhile1
	   -> Expr

   DoSpec
	   -> DoSpecRef '=' DoSpecList

   DoSpecRef
	   -> Reference

   DoSpecList
	   -> DoSpecItem
	   -> DoSpecList ',' DoSpecItem

   DoSpecItem
	   -> DoSpec1 DoSpec2 DoWhile
	   -> DoSpec1 DoSpec2
	   -> DoSpec1         DoWhile

   DoSpec1
	   -> Expr

   DoSpec2
	   -> DoToBy
	   -> DoRepeat

   DoToBy
	   -> DoTo DoBy
	   -> DoTo
	   -> DoBy
	   -> DoBy DoTo

   DoTo
	   -> TO DoTo1

   DoTo1
	   -> Expr

   DoBy
	   -> BY DoBy1

   DoBy1
	   -> Expr

   DoRepeat
	   -> REPEAT DoRep1

   DoRep1
	   -> Expr

   DoDone
	   -> ';'



   /**********/
   /*        */
   /*  END   */
   /*        */
   /**********/

   EndStmt
	   -> EndInit EndBody EndDone
	   -> EndInit         EndDone

   EndInit
	   -> END

   EndBody
	   -> <identifier>

   EndDone
	   -> ';'



   /************/
   /*          */
   /*  ENTRY   */
   /*          */
   /************/

   EntryStmt
	   -> EntryInit EntryBody EntryDone

   EntryInit
	   -> ENTRY

   EntryBody
	   -> EntryInfo1 EntryInfo2 EntryInfo3 EntryInfo4

   EntryInfo1
	   ->
	   -> '(' EntryParmList ')'

   EntryParmList
	   -> EntryParmItem
	   -> EntryParmList ',' EntryParmItem

   EntryParmItem
	   -> <identifier>

   EntryInfo2
	   ->
	   -> EntryRtn1 '(' EntryRtn2 ')'
	   -> EntryRtn1

   EntryRtn1
	   -> RETURNS

   EntryRtn2
	   -> EntryRtn3
	   -> EntryRtn2 ',' EntryRtn3

   EntryRtn3
	   -> EntryRtn4 '(' EntryRtn5 ')' EntryRtn7
	   -> EntryRtn4 '(' EntryRtn5 ')'
	   -> EntryRtn4
	   ->           '(' EntryRtn5 ')' EntryRtn7
	   ->           '(' EntryRtn5 ')'
	   ->                             EntryRtn7

   EntryRtn4
	   -> <integer>

   EntryRtn5
	   -> EntryRtn6
	   -> EntryRtn5 ',' EntryRtn6

   EntryRtn6
	   -> BoundPair

   EntryRtn7
	   -> EntryRtn8
	   -> EntryRtn7 EntryRtn8

   EntryRtn8
	   -> DataAttr

   EntryInfo3
	   ->
	   -> EntryOpt1 '(' EntryOpt2 ')'

   EntryOpt1
	   -> OPTIONS

   EntryOpt2
	   -> EntryOpt3
	   -> EntryOpt2 ',' EntryOpt3

   EntryOpt3
	   -> <identifier>

   EntryInfo4
	   ->
	   -> RECURSIVE

   EntryDone
	   -> ';'



   /***********/
   /*         */
   /*  EXEC   */
   /*         */
   /***********/

   ExecStmt
	   -> ExecInit ExecBody ExecDone

   ExecInit
	   -> EXEC

   ExecBody
	   -> CICS

   ExecDone
	   -> ';'



   /*************/
   /*           */
   /*  FORMAT   */
   /*           */
   /*************/

   FormatStmt
	   -> FormatInit '(' FormatList ')' FormatDone

   FormatInit
	   -> FORMAT

   FormatList
	   -> FormatItem
	   -> FormatList ',' FormatItem

   FormatItem
	   -> FmtSpec

   FormatDone
	   -> ';'



   /***********/
   /*         */
   /*  FREE   */
   /*         */
   /***********/

   FreeStmt
	   -> FreeInit FreeList FreeDone

   FreeInit
	   -> FREE

   FreeList
	   -> FreeItem
	   -> FreeList ',' FreeItem

   FreeItem
	   -> FreeRef FreeIn
	   -> FreeRef

   FreeRef
	   -> Reference

   FreeIn
	   -> IN '(' FreeIn1 ')'

   FreeIn1
	   -> Reference

   FreeDone
	   -> ';'



   /**********/
   /*        */
   /*  GET   */
   /*        */
   /**********/

   GetStmt
	   -> GetInit GetBody GetDone

   GetInit
	   -> GET

   GetBody
	   -> GetFile GetCopy GetSkip GetIpt
	   -> GetFile GetCopy GetSkip
	   -> STRING '(' GetStr1 ')' GetIpt GetCopy

   GetFile
	   ->
	   -> FILE '(' GetFile1 ')'

   GetFile1
	   -> Reference

   GetCopy
	   ->
	   -> COPY '(' GetCopy1 ')'
	   -> COPY

   GetCopy1
	   -> Reference

   GetSkip
	   ->
	   -> SKIP '(' GetSkip1 ')'
	   -> SKIP

   GetSkip1
	   -> Expr

   GetStr1
	   -> Expr

   GetIpt
	   -> GetData
	   -> GetList
	   -> GetEdit

   GetData
	   -> DATA '(' GetDataList ')'
	   -> DATA

   GetDataList
	   -> GetDataItem
	   -> GetDataList ',' GetDataItem

   GetDataItem
	   -> GetDataNameList

   GetDataNameList
	   -> GetDataNameItem
	   -> GetDataNameList '.' GetDataNameItem

   GetDataNameItem
	   -> <identifier>

   GetList
	   -> LIST '(' GetTargList ')'

   GetTargList
	   -> GetTargItem
	   -> GetTargList ',' GetTargItem

   GetTargItem
	   -> Reference
	   -> GetTarg1 GetTarg2

   GetTarg1
	   -> '(' GetTargList ')'

   GetTarg2
	   -> DO GetDo

   GetDo
	   -> GetDoRef '=' GetDoList

   GetDoRef
	   -> Reference

   GetDoList
	   -> GetDoItem
	   -> GetDoList ',' GetDoItem

   GetDoItem
	   -> GetDoItem1 GetDoItem2 GetDoItem3
	   -> GetDoItem1 GetDoItem2
	   -> GetDoItem1            GetDoItem3

   GetDoItem1
	   -> Expr

   GetDoItem2
	   -> GetDoToBy
	   -> GetDoRepeat

   GetDoItem3
	   -> WHILE '(' GetDoWhile ')'

   GetDoWhile
	   -> Expr

   GetDoToBy
	   -> GetDoTo GetDoBy
	   -> GetDoTo
	   -> GetDoBy
	   -> GetDoBy GetDoTo

   GetDoTo
	   -> TO GetDoTo1

   GetDoTo1
	   -> Expr

   GetDoBy
	   -> BY GetDoBy1

   GetDoBy1
	   -> Expr

   GetDoRepeat
	   -> REPEAT GetDoRep1

   GetDoRep1
	   -> Expr

   GetEdit
	   -> EDIT GetEditList1

   GetEditList1
	   -> GetEditItem1
	   -> GetEditList1 ',' GetEditItem1

   GetEditItem1
	   -> '(' GetTargList ')' '(' GetEditList2 ')'

   GetEditList2
	   -> GetEditItem2
	   -> GetEditList2 ',' GetEditItem2

   GetEditItem2
	   -> FmtSpec

   GetDone
	   -> ';'



   /***********/
   /*         */
   /*  GOTO   */
   /*         */
   /***********/

   GotoStmt
	   -> GotoInit GotoBody GotoDone

   GotoInit
	   -> GOTO
	   -> GO TO

   GotoBody
	   -> Reference

   GotoDone
	   -> ';'



   /*********/
   /*       */
   /*  IF   */
   /*       */
   /*********/

   IfStmt
	   -> IfClause IfExecUnit
	   -> IfClause BalUnit IfElse IfExecUnit

   IfClause
	   -> IfInit IfExpr IfThen

   IfInit
	   -> IF

   IfExpr
	   -> Expr

   IfThen
	   -> THEN

   IfElse
	   -> ELSE

   IfExecUnit
	   -> ExecUnit



   /*************/
   /*           */
   /*  LOCATE   */
   /*           */
   /*************/

   LocateStmt
	   -> LocInit LocName LocTail LocDone

   LocInit
	   -> LOCATE

   LocName
	   -> <identifier>

   LocTail
	   -> LocFile LocSet LocKey

   LocFile
	   -> FILE '(' LocFile1 ')'

   LocFile1
	   -> Reference

   LocSet
	   ->
	   -> SET '(' LocSet1 ')'

   LocSet1
	   -> Reference

   LocKey
	   ->
	   -> KEYFROM '(' LocKey1 ')'

   LocKey1
	   -> Expr

   LocDone
	   -> ';'



   /***********/
   /*         */
   /*  NULL   */
   /*         */
   /***********/

   NullStmt
	   -> ';'



   /*********/
   /*       */
   /*  ON   */
   /*       */
   /*********/

   OnStmt
	   -> OnInit OnList OnSnap OnTail

   OnInit
	   -> ON

   OnList
	   -> OnItem
	   -> OnList ',' OnItem

   OnItem
	   -> CondName

   OnSnap
	   ->
	   -> SNAP

   OnTail
	   -> SYSTEM ';'
	   -> '(' CondPfixList ')' ':' OnExecStmt
	   ->                          OnExecStmt
	   -> '(' CondPfixList ')' ':' OnBlock
	   ->                          OnBlock

   OnExecStmt
	   -> ExecSnglStmt

   OnBlock
	   -> Block



   /***********/
   /*         */
   /*  OPEN   */
   /*         */
   /***********/

   OpenStmt
	   -> OpenInit OpenList1 OpenDone

   OpenInit
	   -> OPEN

   OpenList1
	   -> OpenItem1
	   -> OpenList1 ',' OpenItem1

   OpenItem1
	   -> OpenList2

   OpenList2
	   -> OpenItem2
	   -> OpenList2 OpenItem2

   OpenItem2
	   -> STREAM
	   -> RECORD
	   -> INPUT
	   -> OUTPUT
	   -> UPDATE
	   -> SEQUENTIAL
	   -> SEQL
	   -> DIRECT
	   -> PRINT
	   -> KEYED
	   -> FILE        '(' OpenFile     ')'
	   -> TAB         '(' OpenTabList  ')'
	   -> TITLE       '(' OpenTitle    ')'
	   -> LINESIZE    '(' OpenLineSize ')'
	   -> PAGESIZE    '(' OpenPageSize ')'
	   -> ENVIRONMENT '(' OpenEnvList  ')'
	   -> ENV         '(' OpenEnvList  ')'

   OpenFile
	   -> Reference

   OpenTabList
	   -> OpenTabItem
	   -> OpenTabList ',' OpenTabItem

   OpenTabItem
	   -> Expr

   OpenTitle
	   -> Expr

   OpenLineSize
	   -> Expr

   OpenPageSize
	   -> Expr

   OpenEnvList
	   -> OpenEnvItem
	   -> OpenEnvList ',' OpenEnvItem

   OpenEnvItem
	   -> <identifier>

   OpenDone
	   -> ';'



   /***********/
   /*         */
   /*  PROC   */
   /*         */
   /***********/

   ProcStmt
	   -> ProcInit ProcBody ProcDone

   ProcInit
	   -> PROCEDURE
	   -> PROC

   ProcBody
	   -> ProcInfo1 ProcInfo2 ProcInfo3 ProcInfo4

   ProcInfo1
	   ->
	   -> '(' ProcParmList ')'

   ProcParmList
	   -> ProcParmItem
	   -> ProcParmList ',' ProcParmItem

   ProcParmItem
	   -> <identifier>

   ProcInfo2
	   ->
	   -> ProcRtn1 '(' ProcRtn2 ')'
	   -> ProcRtn1

   ProcRtn1
	   -> RETURNS

   ProcRtn2
	   -> ProcRtn3
	   -> ProcRtn2 ',' ProcRtn3

   ProcRtn3
	   -> ProcRtn4 '(' ProcRtn5 ')' ProcRtn7
	   -> ProcRtn4 '(' ProcRtn5 ')'
	   -> ProcRtn4
	   ->          '(' ProcRtn5 ')' ProcRtn7
	   ->          '(' ProcRtn5 ')'
	   ->                           ProcRtn7

   ProcRtn4
	   -> <integer>

   ProcRtn5
	   -> ProcRtn6
	   -> ProcRtn5 ',' ProcRtn6

   ProcRtn6
	   -> BoundPair

   ProcRtn7
	   -> ProcRtn8
	   -> ProcRtn7 ProcRtn8

   ProcRtn8
	   -> DataAttr

   ProcInfo3
	   ->
	   -> ProcOpt1 '(' ProcOpt2 ')'

   ProcOpt1
	   -> OPTIONS

   ProcOpt2
	   -> ProcOpt3
	   -> ProcOpt2 ',' ProcOpt3

   ProcOpt3
	   -> <identifier>

   ProcInfo4
	   ->
	   -> RECURSIVE

   ProcDone
	   -> ';'



   /*********/
   /*       */
   /*  PUT  */
   /*       */
   /*********/

   PutStmt
	   -> PutInit PutBody PutDone

   PutInit
	   -> PUT

   PutBody
	   -> PutFile PutSkip PutLine PutPage PutOut
	   -> PutFile PutSkip PutLine PutPage
	   -> STRING '(' PutStr1 ')' PutOut

   PutFile
	   ->
	   -> FILE '(' PutFile1 ')'

   PutFile1
	   -> Reference

   PutSkip
	   ->
	   -> SKIP '(' PutSkip1 ')'
	   -> SKIP

   PutSkip1
	   -> Expr

   PutLine
	   ->
	   -> LINE '(' PutLine1 ')'

   PutLine1
	   -> Expr

   PutPage
	   ->
	   -> PAGE

   PutStr1
	   -> Expr

   PutOut
	   -> PutData
	   -> PutList
	   -> PutEdit

   PutData
	   -> DATA '(' PutDataList ')'
	   -> DATA

   PutDataList
	   -> PutDataItem
	   -> PutDataList ',' PutDataItem

   PutDataItem
	   -> Reference
	   -> PutData1 PutData2

   PutData1
	   -> '(' PutDataList ')'

   PutData2
	   -> DO PutDo

   PutDo
	   -> PutDoRef '=' PutDoList

   PutDoRef
	   -> Reference

   PutDoList
	   -> PutDoItem
	   -> PutDoList ',' PutDoItem

   PutDoItem
	   -> PutDoItem1 PutDoItem2 PutDoItem3
	   -> PutDoItem1 PutDoItem2
	   -> PutDoItem1            PutDoItem3

   PutDoItem1
	   -> Expr

   PutDoItem2
	   -> PutDoToBy
	   -> PutDoRepeat

   PutDoItem3
	   -> WHILE '(' PutDoWhile ')'

   PutDoWhile
	   -> Expr

   PutDoToBy
	   -> PutDoTo PutDoBy
	   -> PutDoTo
	   -> PutDoBy
	   -> PutDoBy PutDoTo

