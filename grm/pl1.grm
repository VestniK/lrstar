   /****************/
   /*              */
   /*  EXPRESSION  */
   /*              */
   /****************/

   Expr
	   -> Expr '|' Expr1
	   ->          Expr1

   Expr1
	   -> Expr1 '&' Expr2
	   ->           Expr2

   Expr2
	   -> Expr2 '>'  Expr3
	   -> Expr2 '>=' Expr3
	   -> Expr2 '='  Expr3
	   -> Expr2 '<'  Expr3
	   -> Expr2 '<=' Expr3
	   -> Expr2 '~>' Expr3
	   -> Expr2 '~=' Expr3
	   -> Expr2 '~<' Expr3
	   ->            Expr3

   Expr3
	   -> Expr3 '||' Expr4
	   ->          Expr4

   Expr4
	   -> Expr4 '+' Expr5
	   -> Expr4 '-' Expr5
	   ->           Expr5

   Expr5
	   -> Expr5 '*' Expr6
	   -> Expr5 '/' Expr6
	   ->           Expr6

   Expr6
	   -> PrimExpr
	   -> PfixExpr
	   -> '(' Expr ')'
	   -> Expr7

   Expr7
	   -> PrimExpr     '**' Expr6
	   -> '(' Expr ')' '**' Expr6

   PfixExpr
	   -> '+' Expr6
	   -> '-' Expr6
	   -> '~' Expr6

   PrimExpr
	   -> Reference
	   -> Constant
	   -> Isub



