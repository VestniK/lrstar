   /*  FLT CON  */
   /*           */
   /*************/

   FltCon
	   -> FltConFix '.' FltConDec E FltConExp
	   -> FltConFix '.' FltConDec
	   -> FltConFix               E FltConExp
	   -> FltConFix '.'
	   ->           '.' FltConDec E FltConExp
	   ->           '.' FltConDec

   FltConFix
	   -> <integer>

   FltConDec
	   -> <integer>

   FltConExp
	   -> <integer>



   /***************/
   /*             */
   /*  QUAL NAME  */
   /*             */
   /***************/

   QualName
	   -> QualNameInit QualNameBase QualNameIndex QualNameRange QualNameDone

   QualNameInit
	   ->

   QualNameBase
	   -> QualNameBase1
	   -> QualNameBase1 '^' QualNameBase2

   QualNameBase1
	   -> BuiltIn
	   -> <identifier>

   QualNameBase2
	   -> <identifier>

   QualNameIndex
	   ->
	   -> QualNameIdxLpar QualNameIdxList QualNameIdxRpar

   QualNameIdxLpar
	   -> '('

   QualNameIdxList
	   -> QualNameIdxItem
	   -> QualNameIdxList ',' QualNameIdxItem

   QualNameIdxItem
	   -> NumExpr

   QualNameIdxRpar
	   -> ')'

   QualNameRange
	   ->
	   -> QualNameRngLpar QualNameRngBody QualNameRngRpar

   QualNameRngLpar
	   -> '('

   QualNameRngBody
	   -> QualNameRngHead QualNameRngMark QualNameRngTail
	   -> QualNameRngHead QualNameRngMark
	   ->                 QualNameRngMark QualNameRngTail
	   ->                 QualNameRngMark

   QualNameRngHead
	   -> NumExpr

   QualNameRngMark
	   -> ':'

   QualNameRngTail
	   -> NumExpr

   QualNameRngRpar
	   -> ')'

   QualNameDone
	   ->



