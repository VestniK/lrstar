
/* FORTRAN 77 */

/* by Ed Dobies
      Xypro Technology
      Simi Valley, CA
      1988 ?
*/

   <error>        => error();
   <identifier>   => lookup();
   <character>    => lookup();
   <integer>      => lookup();
   <string>       => lookup();
   <label>        => lookup();
   <eol>				;
   <eof>				;

/*  FORTRAN Grammar  */

   Start
	   -> TranUnit <eof>

   TranUnit
	   ->
	   -> ProgList

   ProgList
	   -> ProgItem
	   -> ProgList ProgItem

   ProgItem
	   -> MainProg
	   -> FuncProg
	   -> SubrProg
	   -> BlockDataProg

   MainProg
	   -> ProgramStmt ProgBody
	   ->             ProgBody

   FuncProg
	   -> FunctionStmt ProgBody

   SubrProg
	   -> SubroutineStmt ProgBody

   BlockDataProg
	   -> BlockDataStmt ProgBody

   ProgBody
	   -> Grp1StmtList Grp2StmtList Grp3StmtList Grp4StmtList ProgEnd

   ProgEnd
	   -> StmtLabel EndStmt
	   ->           EndStmt

   StmtLabel
	   -> <label>

   Grp1StmtList
	   ->
	   -> Grp1StmtList Grp1StmtItem

   Grp1StmtItem
	   -> StmtLabel FormatStmt
	   -> StmtLabel EntryStmt
	   ->           EntryStmt
	   -> StmtLabel ImplicitStmt
	   ->           ImplicitStmt
	   -> StmtLabel ParameterStmt
	   ->           ParameterStmt

   Grp2StmtList
	   ->
	   -> Grp2StmtList Grp2StmtItem

   Grp2StmtItem
	   -> StmtLabel FormatStmt
	   -> StmtLabel CommonStmt
	   ->           CommonStmt
	   -> StmtLabel DimensionStmt
	   ->           DimensionStmt
	   -> StmtLabel EndRecordStmt
	   ->           EndRecordStmt
	   -> StmtLabel EntryStmt
	   ->           EntryStmt
	   -> StmtLabel EquivalenceStmt
	   ->           EquivalenceStmt
	   -> StmtLabel ExternalStmt
	   ->           ExternalStmt
	   -> StmtLabel IntrinsicStmt
	   ->           IntrinsicStmt
	   -> StmtLabel ParameterStmt
	   ->           ParameterStmt
	   -> StmtLabel RecordStmt
	   ->           RecordStmt
	   -> StmtLabel SaveStmt
	   ->           SaveStmt
	   -> StmtLabel TypeStmt
	   ->           TypeStmt

   Grp3StmtList
	   ->
	   -> Grp3StmtList Grp3StmtItem

   Grp3StmtItem
	   -> StmtLabel FormatStmt
	   -> StmtLabel DataStmt
	   ->           DataStmt
	   -> StmtLabel EntryStmt
	   ->           EntryStmt

   Grp4StmtList
	   ->
	   -> Grp4StmtList Grp4StmtItem

   Grp4StmtItem
	   -> StmtLabel FormatStmt
	   -> StmtLabel DataStmt
	   ->           DataStmt
	   -> StmtLabel EntryStmt
	   ->           EntryStmt
	   -> StmtLabel ExecutableStmt
	   ->           ExecutableStmt

   ExecutableStmt
	   -> AssignmentStmt
	   -> BackspaceStmt
	   -> CallStmt
	   -> CheckpointStmt
	   -> CloseStmt
	   -> ContinueStmt
	   -> DoStmt
	   -> ElseStmt
	   -> ElseIfStmt
	   -> EndfileStmt
	   -> EndIfStmt
	   -> GoToStmt
	   -> IfStmt
	   -> InquireStmt
	   -> OpenStmt
	   -> PauseStmt
	   -> PositionStmt
	   -> PrintStmt
	   -> ReadStmt
	   -> ReceiveStmt
	   -> ReturnStmt
	   -> RewindStmt
	   -> StartbackupStmt
	   -> StopStmt
	   -> UnitStmt
	   -> WriteStmt



   /****************/
   /*              */
   /*  ASSIGNMENT  */
   /*              */
   /****************/

   AssignmentStmt
	   -> AsgnValStmt AsgnDone
	   -> AsgnAdrStmt AsgnDone

   AsgnValStmt
	   -> AsgnLHS '=' AsgnRHS

   AsgnLHS
	   -> QualName

   AsgnRHS
	   -> AnyExpr

   AsgnAdrStmt
	   -> AsgnInit AsgnLabel TO AsgnName

   AsgnInit
	   -> ASSIGN

   AsgnLabel
	   -> <integer>

   AsgnName
	   -> <identifier>

   AsgnDone
	   -> <eol>



   /***************/
   /*             */
   /*  BACKSPACE  */
   /*             */
   /***************/

   BackspaceStmt
	   -> BkspStmtInit   BkspUnit       BkspStmtDone
	   -> BkspStmtInit '(' BkspStmtBody ')' BkspStmtDone

   BkspStmtInit
	   -> BACKSPACE

   BkspStmtBody
	   -> BkspUnit
	   -> BkspUnit ',' BkspCtrlList
	   ->              BkspCtrlList

   BkspCtrlList
	   -> BkspCtrlItem
	   -> BkspCtrlList ',' BkspCtrlItem

   BkspCtrlItem
	   -> UNIT   '=' BkspUnit
	   -> ERR    '=' BkspErr
	   -> IOSTAT '=' BkspIOStat

   BkspUnit
	   -> NumExpr
	   -> '*'

   BkspErr
	   -> <integer>

   BkspIOStat
	   -> QualName

   BkspStmtDone
	   -> <eol>



   /****************/
   /*              */
   /*  BLOCK DATA  */
   /*              */
   /****************/

   BlockDataStmt
	   -> BlockDataStmtInit BlockDataProgName BlockDataStmtDone

   BlockDataStmtInit
	   -> BLOCK DATA

   BlockDataProgName
	   -> <identifier>

   BlockDataStmtDone
	   -> <eol>



   /**********/
   /*        */
   /*  CALL  */
   /*        */
   /**********/

   CallStmt
	   -> CallStmtInit CallStmtSubr CallStmtBody CallStmtDone

   CallStmtInit
	   -> CALL

   CallStmtSubr
	   -> <identifier>

   CallStmtBody
	   ->
	   -> '(' CallArgList ')'
	   -> '('             ')'

   CallArgList
	   -> CallArgItem
	   -> CallArgList ',' CallArgItem

   CallArgItem
	   -> AnyExpr
	   -> '*' CallLabel

   CallLabel
	   -> <integer>

   CallStmtDone
	   -> <eol>



   /***********/
   /*         */
   /*  CLOSE  */
   /*         */
   /***********/

   CloseStmt
	   -> CloseStmtInit '(' CloseStmtBody ')' CloseStmtDone

   CloseStmtInit
	   -> CLOSE

   CloseStmtBody
	   -> CloseUnit
	   -> CloseUnit ',' CloseCtrlList
	   ->               CloseCtrlList

   CloseCtrlList
	   -> CloseCtrlItem
	   -> CloseCtrlList ',' CloseCtrlItem

   CloseCtrlItem
	   -> UNIT   '=' CloseUnit
	   -> ERR    '=' CloseErr
	   -> STATUS '=' CloseStatus
	   -> IOSTAT '=' CloseIOStat
	   -> STACK  '=' CloseStack

   CloseUnit
	   -> NumExpr
	   -> '*'

   CloseErr
	   -> <integer>

   CloseStatus
	   -> ChrExpr

   CloseIOStat
	   -> ChrExpr

   CloseStack
	   -> ChrExpr

   CloseStmtDone
	   -> <eol>



   /************/
   /*          */
   /*  COMMON  */
   /*          */
   /************/

   CommonStmt
	   -> ComStmtInit CommonBody ComStmtDone

   ComStmtInit
	   -> COMMON

   CommonBody
	   -> CommonList1
	   -> CommonList1 ',' CommonList2
	   -> CommonList1     CommonList2
	   ->                 CommonList2

   CommonList1
	   -> CommonItem1
	   -> CommonList1 ',' CommonItem1

   CommonItem1
	   -> <identifier>
	   -> ArrayDecl

   CommonList2
	   -> CommonItem2
	   -> CommonList2 ',' CommonItem2
	   -> CommonList2     CommonItem2

   CommonItem2
	   -> CommonHead CommonList1

   CommonHead
	   -> '/' ComBlockName '/'
	   -> '/'              '/'

   ComBlockName
	   -> <identifier>

   ComStmtDone
	   -> <eol>



   /****************/
   /*              */
   /*  CHECKPOINT  */
   /*              */
   /****************/

   CheckpointStmt
	   -> CkptStmtInit CkptStmtBody CkptStmtDone

   CkptStmtInit
	   -> CKECKPOINT

   CkptStmtBody
	   ->
	   -> '(' CkptCtrlList ')' CkptDataList
	   -> '(' CkptCtrlList ')'
	   ->                  CkptDataList

   CkptCtrlList
	   -> CkptCtrlItem
	   -> CkptCtrlList ',' CkptCtrlItem

   CkptCtrlItem
	   -> UNIT         '=' CkptUnit
	   ->                  CkptUnit
	   -> FILENUM      '=' CkptFileNum
	   -> ERR          '=' CkptErr
	   -> BACKUPSTATUS '=' CkptBupStat
	   -> STACK        '=' CkptStack

   CkptUnit
	   -> NumExpr

   CkptFileNum
	   -> NumExpr

   CkptErr
	   -> <integer>

   CkptBupStat
	   -> QualName

   CkptStack
	   -> ChrExpr

   CkptDataList
	   -> CkptDataItem
	   -> CkptDataList ',' CkptDataItem

   CkptDataItem
	   -> QualName

   CkptStmtDone
	   -> <eol>



   /**************/
   /*            */
   /*  CONTINUE  */
   /*            */
   /**************/

   ContinueStmt
	   -> ContStmtInit ContStmtDone

   ContStmtInit
	   -> CONTINUE

   ContStmtDone
	   -> <eol>



   /**********/
   /*        */
   /*  DATA  */
   /*        */
   /**********/

   DataStmt
	   -> DataStmtInit DataBodyList DataStmtDone

   DataStmtInit
	   -> DATA

   DataBodyList
	   -> DataBodyItem
	   -> DataBodyList ',' DataBodyItem
	   -> DataBodyList     DataBodyItem

   DataBodyItem
	   -> DataHeadList DataHeadMark DataTailList DataTailMark

   DataHeadList
	   -> DataHeadItem
	   -> DataHeadList ',' DataHeadItem

   DataHeadItem
	   -> QualName
	   -> DataImpDo

   DataHeadMark
	   -> '/'

   DataTailList
	   -> DataTailItem
	   -> DataTailList ',' DataTailItem

   DataTailItem
	   -> DataTailItem1 '*' DataTailItem2
	   ->                   DataTailItem2

   DataTailItem1
	   -> <integer>
	   -> <identifier>

   DataTailItem2
	   -> AnyCon
	   -> <identifier>

   DataTailMark
	   -> '/'

   DataImpDo
	   -> DataDoLpar DataDoList1 ',' DataDoIdx '=' DataDoList2 DataDoRpar

   DataDoLpar
	   -> '('

   DataDoList1
	   -> DataDoItem1
	   -> DataDoList1 ',' DataDoItem1

   DataDoItem1
	   -> QualName
	   -> DataImpDo

   DataDoIdx
	   -> <identifier>

   DataDoList2
	   -> DataDoItem2
	   -> DataDoList2 ',' DataDoItem2

   DataDoItem2
	   -> IntConExpr

   DataDoRpar
	   -> ')'

   DataStmtDone
	   -> <eol>



   /***************/
   /*             */
   /*  DIMENSION  */
   /*             */
   /***************/

   DimensionStmt
	   -> DimStmtInit DimDeclList DimStmtDone

   DimStmtInit
	   -> DIMENSION

   DimDeclList
	   -> DimDeclItem
	   -> DimDeclList ',' DimDeclItem

   DimDeclItem
	   -> ArrayDecl

   DimStmtDone
	   -> <eol>



   /********/
   /*      */
   /*  DO  */
   /*      */
   /********/

   DoStmt
	   -> DoStmtInit DoStmtBody DoStmtDone

   DoStmtInit
	   -> DO

   DoStmtBody
	   -> DoLabel ',' DoIndex '=' DoTailList
	   -> DoLabel     DoIndex '=' DoTailList

   DoLabel
	   -> <integer>

   DoIndex
	   -> <identifier>

   DoTailList
	   -> DoTailItem
	   -> DoTailList ',' DoTailItem

   DoTailItem
	   -> NumExpr

   DoStmtDone
	   ->



   /**********/
   /*        */
   /*  ELSE  */
   /*        */
   /**********/

   ElseStmt
	   -> ElseStmtInit ElseStmtDone

   ElseStmtInit
	   -> ELSE

   ElseStmtDone
	   -> <eol>



   /*************/
   /*           */
   /*  ELSE IF  */
   /*           */
   /*************/

   ElseIfStmt
	   -> ElseIfInit '(' ElseIfPred ')' THEN ElseIfDone

   ElseIfInit
	   -> ELSE IF

   ElseIfPred
	   -> AnyExpr

   ElseIfDone
	   -> <eol>



   /*********/
   /*       */
   /*  END  */
   /*       */
   /*********/

   EndStmt
	   -> EndStmtSpec EndStmtDone

   EndStmtSpec
	   -> END

   EndStmtDone
	   -> <eol>



   /***************/
   /*             */
   /*  END RECORD */
   /*             */
   /***************/

   EndRecordStmt
	   -> EndRecStmtSpec EndRecStmtDone

   EndRecStmtSpec
	   -> END RECORD

   EndRecStmtDone
	   -> <eol>



   /*************/
   /*           */
   /*  ENDFILE  */
   /*           */
   /*************/

   EndfileStmt
	   -> EndfStmtInit   EndfUnit       EndfStmtDone
	   -> EndfStmtInit '(' EndfStmtBody ')' EndfStmtDone

   EndfStmtInit
	   -> ENDFILE

   EndfStmtBody
	   -> EndfUnit
	   -> EndfUnit ',' EndfCtrlList
	   ->              EndfCtrlList

   EndfCtrlList
	   -> EndfCtrlItem
	   -> EndfCtrlList ',' EndfCtrlItem

   EndfCtrlItem
	   -> UNIT   '=' EndfUnit
	   -> ERR    '=' EndfErr
	   -> IOSTAT '=' EndfIOStat

   EndfUnit
	   -> NumExpr
	   -> '*'

   EndfErr
	   -> <integer>

   EndfIOStat
	   -> QualName

   EndfStmtDone
	   -> <eol>



   /************/
   /*          */
   /*  END IF  */
   /*          */
   /************/

   EndIfStmt
	   -> EndIfInit EndIfDone

   EndIfInit
	   -> END IF

   EndIfDone
	   -> <eol>



   /***********/
   /*         */
   /*  ENTRY  */
   /*         */
   /***********/

   EntryStmt
	   -> EntryStmtInit EntryName EntryParams EntryStmtDone

   EntryStmtInit
	   -> ENTRY

   EntryName
	   -> <identifier>

   EntryParams
	   ->
	   -> '(' EntryParamList ')'

   EntryParamList
	   ->
	   -> EntryParamList ',' EntryParamItem

   EntryParamItem
	   -> '*'
	   -> <identifier>

   EntryStmtDone
	   -> <eol>



   /*****************/
   /*               */
   /*  EQUIVALENCE  */
   /*               */
   /*****************/

   EquivalenceStmt
	   -> EquivStmtInit EquivBodyList EquivStmtDone

   EquivStmtInit
	   -> EQUIVALENCE

   EquivBodyList
	   -> EquivBodyItem
	   -> EquivBodyList ',' EquivBodyItem

   EquivBodyItem
	   -> EquivLpar EquivNameItem ',' EquivNameList EquivRpar

   EquivLpar
	   -> '('

   EquivNameList
	   -> EquivNameItem
	   -> EquivNameList ',' EquivNameItem

   EquivNameItem
	   -> QualName

   EquivRpar
	   -> ')'

   EquivStmtDone
	   -> <eol>



   /**************/
   /*            */
   /*  EXTERNAL  */
   /*            */
   /**************/

   ExternalStmt
	   -> ExternStmtInit ExternBodyList ExternStmtDone

   ExternStmtInit
	   -> EXTERNAL

   ExternBodyList
	   -> ExternBodyItem
	   -> ExternBodyList ',' ExternBodyItem

   ExternBodyItem
	   -> <identifier>

   ExternStmtDone
	   -> <eol>



   /************/
   /*          */
   /*  FORMAT  */
   /*          */
   /************/

   FormatStmt
	   -> FmtStmtInit FmtStmtBody FmtStmtDone

   FmtStmtInit
	   -> FORMAT

   FmtStmtBody
	   -> '(' FmtSpecList ')'
	   -> '('             ')'

   FmtSpecList
	   -> FmtSpecItem
	   -> FmtSpecList ',' FmtSpecItem
	   -> FmtSpecList     FmtSpecItem

   FmtSpecItem
	   -> FmtRepeat FmtSpec1 FmtSpec2
	   ->           FmtSpec1 FmtSpec2
	   -> FmtRepeat FmtSpec1 FmtSpec2 FmtSpec5
	   ->           FmtSpec1 FmtSpec2 FmtSpec5
	   -> FmtRepeat FmtSpec2
	   ->           FmtSpec2
	   -> FmtRepeat FmtSpec2 FmtSpec5
	   ->           FmtSpec2 FmtSpec5
	   -> FmtRepeat FmtSpec3
	   ->           FmtSpec3
	   -> FmtRepeat FmtSpec3 FmtSpec5
	   ->           FmtSpec3 FmtSpec5
	   -> FmtRepeat FmtSpec4
	   ->           FmtSpec4
	   -> FmtRepeat FmtSpec4 FmtSpec5
	   ->           FmtSpec4 FmtSpec5
	   -> FmtRepeat FmtSpec5
	   ->           FmtSpec5

   FmtSpec1
	   -> FmtSpecK P

   FmtSpec2
	   -> F FmtSpecW '.' FmtSpecD
	   -> E FmtSpecW '.' FmtSpecD
	   -> E FmtSpecW '.' FmtSpecD E FmtSpecE
	   -> D FmtSpecW '.' FmtSpecD
	   -> G FmtSpecW '.' FmtSpecD
	   -> G FmtSpecW '.' FmtSpecD E FmtSpecE

   FmtSpec3
	   -> '(' FmtSpecList ')'
	   -> I FmtSpecW
	   -> I FmtSpecW '.' FmtSpecM
	   -> A
	   -> A FmtSpecW
	   -> L FmtSpecW

   FmtSpec4
	   ->            HolorithList
	   -> FmtSpecN H HolorithList
	   -> FmtSpecN X
	   -> T   FmtSpecC
	   -> T L FmtSpecC
	   -> T R FmtSpecC
	   -> S
	   -> S P
	   -> S S
	   -> B N
	   -> B Z

   FmtSpec5
	   -> '/'
	   -> ':'

   HolorithList
	   -> HolorithItem
	   -> HolorithList HolorithItem

   HolorithItem
	   -> FmtSpecH

   FmtRepeat
	   -> <integer>

   FmtSpecW
	   -> <integer>

   FmtSpecE
	   -> <integer>

   FmtSpecN
	   -> <integer>

   FmtSpecC
	   -> <integer>

   FmtSpecD
	   -> <integer>

   FmtSpecM
	   -> <integer>

   FmtSpecK
	   -> '+' <integer>
	   -> '-' <integer>
	   ->     <integer>

   FmtSpecH
	   -> <character>

   FmtStmtDone
	   -> <eol>



   /**************/
   /*            */
   /*  FUNCTION  */
   /*            */
   /**************/

   FunctionStmt
	   -> FuncStmtType FuncStmtInit FuncProgName FuncParams FuncStmtDone

   FuncStmtType
	   -> REAL
	   -> DOUBLE PRECISION
	   -> COMPLEX
	   -> LOGICAL
	   -> FuncStmtInt
	   -> FuncStmtChar

   FuncStmtInt
	   -> FuncStmtInt1
	   -> FuncStmtInt1 '*' FuncStmtInt2

   FuncStmtInt1
	   -> INTEGER

   FuncStmtInt2
	   -> <integer>

   FuncStmtChar
	   -> FuncStmtChar1
	   -> FuncStmtChar1 '*' FuncStmtChar2

   FuncStmtChar1
	   -> CHARACTER

   FuncStmtChar2
	   -> <integer>
	   -> '(' IntConExpr ')'
	   -> '(' '*' ')'

   FuncStmtInit
	   -> FUNCTION

   FuncProgName
	   -> <identifier>

   FuncParams
	   ->
	   -> '(' FuncParamList ')'

   FuncParamList
	   ->
	   -> FuncParamList ',' FuncParamItem

   FuncParamItem
	   -> <identifier>

   FuncStmtDone
	   -> <eol>



   /***********/
   /*         */
   /*  GO TO  */
   /*         */
   /***********/

   GoToStmt
	   -> GoToStmtInit GoToStmtBody GoToStmtDone

   GoToStmtInit
	   -> GO TO

   GoToStmtBody
	   -> GoToUncond
	   -> GoToComputed
	   -> GoToAssigned

   GoToUncond
	   -> GoToLabelItem

   GoToComputed
	   -> '(' GoToLabelList ')' ',' GoToExpr
	   -> '(' GoToLabelList ')'     GoToExpr

   GoToExpr
	   -> NumExpr

   GoToAssigned
	   -> GoToAsgnName ',' '(' GoToLabelList ')'
	   -> GoToAsgnName     '(' GoToLabelList ')'
	   -> GoToAsgnName

   GoToAsgnName
	   -> <identifier>

   GoToLabelList
	   -> GoToLabelItem
	   -> GoToLabelList ',' GoToLabelItem

   GoToLabelItem
	   -> <integer>

   GoToStmtDone
	   -> <eol>



   /********/
   /*      */
   /*  IF  */
   /*      */
   /********/

   IfStmt
	   -> IfStmtInit '(' IfPredicate ')' IfStmtTail

   IfStmtInit
	   -> IF

   IfPredicate
	   -> AnyExpr

   IfStmtTail
	   -> IfTargLabel ',' IfTargLabel ',' IfTargLabel IfStmtDone
	   -> IfStmtExec ExecutableStmt
	   -> THEN IfStmtDone

   IfTargLabel
	   -> <integer>

   IfStmtExec
	   ->

   IfStmtDone
	   -> <eol>



   /**************/
   /*            */
   /*  IMPLICIT  */
   /*            */
   /**************/

   ImplicitStmt
	   -> ImplStmtInit ImplBodyList ImplStmtDone

   ImplStmtInit
	   -> IMPLICIT

   ImplBodyList
	   -> ImplBodyItem
	   -> ImplBodyList ',' ImplBodyItem

   ImplBodyItem
	   -> ImplItemHead ImplItemTail

   ImplItemHead
	   -> REAL
	   -> DOUBLE PRECISION
	   -> COMPLEX
	   -> LOGICAL
	   -> ImplIntItem
	   -> ImplCharItem

   ImplIntItem
	   -> ImplIntType
	   -> ImplIntType '*' ImplIntSize

   ImplIntType
	   -> INTEGER

   ImplIntSize
	   -> INTEGER

   ImplCharItem
	   -> ImplCharType
	   -> ImplCharType '*' ImplCharLen

   ImplCharType
	   -> CHARACTER

   ImplCharLen
	   -> <integer>
	   -> '(' IntConExpr ')'
	   -> '(' '*' ')'

   ImplItemTail
	   -> ImplTailLpar ImplTailList ImplTailRpar

   ImplTailLpar
	   -> '('

   ImplTailList
	   -> ImplTailItem
	   -> ImplTailList ',' ImplTailItem

   ImplTailItem
	   -> ImplSingle
	   -> ImplRange

   ImplRange
	   -> ImplSingle ImplDash ImplSingle

   ImplDash
	   -> '-'

   ImplSingle
	   -> <character>

   ImplTailRpar
	   -> ')'

   ImplStmtDone
	   -> <eol>



   /*************/
   /*           */
   /*  INQUIRE  */
   /*           */
   /*************/

   InquireStmt
	   -> InqStmtInit '(' InqStmtBody ')' InqStmtDone

   InqStmtInit
	   -> INQUIRE

   InqStmtBody
	   -> InqUnit
	   -> InqUnit ',' InqCtrlList
	   ->             InqCtrlList

   InqCtrlList
	   -> InqCtrlItem
	   -> InqCtrlList ',' InqCtrlItem

   InqCtrlItem
	   -> UNIT        '=' InqUnit
	   -> FILE        '=' InqFile
	   -> ERR         '=' InqErr
	   -> IOSTAT      '=' InqIOStat
	   -> EXIST       '=' InqExist
	   -> OPENED      '=' InqOpened
	   -> NUMBER      '=' InqNumber
	   -> NAMED       '=' InqNamed
	   -> NAME        '=' InqName
	   -> ACCESS      '=' InqAccess
	   -> SEQUENTIAL  '=' InqSequential
	   -> DIRECT      '=' InqDirect
	   -> FORM        '=' InqForm
	   -> FORMATTED   '=' InqFormatted
	   -> UNFORMATTED '=' InqUnformatted
	   -> RECL        '=' InqRecl
	   -> NEXTREC     '=' InqNextrec
	   -> BLANK       '=' InqBlank

   InqUnit
	   -> NumExpr
	   -> '*'

   InqFile
	   -> ChrExpr

   InqErr
	   -> <integer>

   InqIOStat
	   -> QualName

   InqExist
	   -> QualName

   InqOpened
	   -> QualName

   InqNumber
	   -> QualName

   InqNamed
	   -> QualName

   InqName
	   -> QualName

   InqAccess
	   -> QualName

   InqSequential
	   -> QualName

   InqDirect
	   -> QualName

   InqForm
	   -> QualName

   InqFormatted
	   -> QualName

   InqUnformatted
	   -> QualName

   InqRecl
